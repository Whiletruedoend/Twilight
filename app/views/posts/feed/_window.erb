<div class="right-bar">
    <%if @posts.count == 0%>
        <article class="feed">
            <center><p><%=I18n.t('feeds.no_results')%></p></center>
        </article>
    <%end%>
    <%@posts.each do |date,posts|%>
        <div class="datestamp"><div class="text-date"><%=date.strftime("%Y-%m-%d")%></div></div>

        <%posts.each do |post|%>
            <article class="feed">
                <div class="user">
                    <div class="icon">
                        <img alt="img" onerror="this.style.display = 'none'" lazy-progressive="true" src=<%=url_for(post.user.avatar.variant(resize_to_limit: [48,48])&.processed)%> lazy="loaded">
                    </div>
                    <div class="name"><%=display_name(post.user)%></div>
                    <div class="createdAt"><%=post.created_at.strftime("%H:%M:%S")%></div>
                    <%= render "privacy-icons", object: post %>
                </div>
                <div class="text">
                    <%= markdown(post.title) %>
                    <%= markdown(post.text) %>
                </div>
                
                <%if post.content_attachments&.any?%>
                    <div class="media-list">
                        <%=display_feed_attachments(post)%>
                    <div>
                    <!---<div class="media-list">
                        <img id="zoom-bg" alt="img" class="original medium-zoom-image" lazy-progressive="true" src="https://i.pinimg.com/originals/fe/14/d9/fe14d91b5495f24825460fd492346886.png" lazy="loaded">
                        <img id="zoom-bg" alt="img" class="original medium-zoom-image" lazy-progressive="true" src="https://cdni.rbth.com/rbthmedia/images/2021.06/article/60bc042485600a38cd63eb89.jpg" lazy="loaded">
                        <img id="zoom-bg" alt="img" class="original medium-zoom-image" lazy-progressive="true" src="https://fretnoize.neocities.org/Images/yummy.jpg" lazy="loaded">
                        <img id="zoom-bg" alt="img" class="original medium-zoom-image" lazy-progressive="true" src="https://www.gannett-cdn.com/presto/2019/07/01/PDTN/1fe778eb-239b-4cc0-bb77-d070591749a1-2019-0701-dg-jail0092.jpg?crop=6015,3372,x0,y0&width=3200&height=1680&fit=bounds" lazy="loaded">
                    </div> --->
                <%end%>
            </article>
        <%end%>
    <%end%>

